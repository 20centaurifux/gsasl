#, c-format
msgid "Choose SASL mechanism:\n"
msgstr "Изабери SASL метод:\n"

#, c-format
msgid "Input list of SASL mechanisms supported by server:\n"
msgstr "Улазни списак SASL метода подржаних од сервера:\n"

#, c-format
msgid "Using mechanism:\n"
msgstr "Користи метод:\n"

#, c-format
msgid "Output from server:\n"
msgstr "Излаз сервера:\n"

#, c-format
msgid "Output from client:\n"
msgstr "Излаз клијента:\n"

msgid "Cannot initialize Windows sockets."
msgstr "Не може иницијализовати Виндоуз сокете."

#, c-format
msgid ""
"missing argument\n"
"Try `%s --help' for more information."
msgstr ""
"недостаје аргумент\n"
"Покушај „%s --help“ за више објашњења."

msgid "need both --x509-cert-file and --x509-key-file"
msgstr "потребни и --x509-cert-file и --x509-key-file"

msgid "cannot use both --starttls and --no-starttls"
msgstr "не може користити и --starttls и --no-starttls"

msgid "cannot use both --smtp and --imap"
msgstr "не може користити и --smtp и --imap"

#, c-format
msgid "initialization failure: %s"
msgstr "иницијализација неуспешна: %s"

#, c-format
msgid "error listing mechanisms: %s"
msgstr "списак грешака метода: %s"

#, c-format
msgid "This client supports the following mechanisms:\n"
msgstr "Овај клијент подржава следеће методе:\n"

#, c-format
msgid "This server supports the following mechanisms:\n"
msgstr "Овај сервер подржава следеће методе:\n"

#, c-format
msgid "GnuTLS global initialization failed: %s"
msgstr "GnuTLS глобална иницијализација неуспешна: %s"

#, c-format
msgid "GnuTLS initialization failed: %s"
msgstr "GnuTLS иницијализација неуспешна: %s"

#, c-format
msgid "setting GnuTLS defaults failed: %s"
msgstr "подразумевано подешавање GnuTLS неуспешно: %s"

#, c-format
msgid "allocating anonymous GnuTLS credential: %s"
msgstr "добављам анонимно GnuTLS уверење: %s"

#, c-format
msgid "setting anonymous GnuTLS credential: %s"
msgstr "задајем анонимно GnuTLS уверење: %s"

#, c-format
msgid "allocating X.509 GnuTLS credential: %s"
msgstr "добављам X.509 GnuTLS уверење: %s"

#, c-format
msgid "loading X.509 GnuTLS credential: %s"
msgstr "учитавам X.509 GnuTLS уверење: %s"

#, c-format
msgid "no X.509 CAs found: %s"
msgstr "ниједан X.509 издавач сертификата нађен: %s"

msgid "no X.509 CAs found"
msgstr "ниједан X.509 издавач сертификата нађен"

#, c-format
msgid "setting X.509 GnuTLS credential: %s"
msgstr "задајем X.509 GnuTLS уверење: %s"

#, c-format
msgid "setting GnuTLS key exchange priority: %s"
msgstr "задајем приоритет измене GnuTLS кључа: %s"

#, c-format
msgid "GnuTLS handshake failed: %s"
msgstr "GnuTLS руковање неуспешно: %s"

#, c-format
msgid "verifying peer certificate: %s"
msgstr "проверавам вршњаково уверење: %s"

msgid "server certificate is not trusted"
msgstr "сертификату сервера се не верује"

msgid "server certificate hasn't got a known issuer"
msgstr "серверов сертификат нема познатог издавача"

msgid "server certificate has been revoked"
msgstr "серверов сертификат је повучен"

#, c-format
msgid "could not verify server certificate (rc=%d)"
msgstr "не може потврдити сертификат сервера (rc=%d)"

#, c-format
msgid "Cannot find mechanism...\n"
msgstr "Не може наћи метод...\n"

#, c-format
msgid "mechanism unavailable: %s"
msgstr "недоступан метод: %s"

#, c-format
msgid "Enter base64 authentication data from client (press RET if none):\n"
msgstr "Унесите base64 податке за потврду клијента (притисните <RET> ако нема):\n"

#, c-format
msgid "Enter base64 authentication data from server (press RET if none):\n"
msgstr "Унесите base64 податке за потврду сервера (притисните <RET> ако нема):\n"

#, c-format
msgid "mechanism error: %s"
msgstr "грешка метода: %s"

#, c-format
msgid "Server authentication finished (client trusted)...\n"
msgstr "Потврђивање сервера завршено (клијенту се верује)...\n"

#, c-format
msgid "Client authentication finished (server trusted)...\n"
msgstr "Потврђивање клијента завршено (серверу се верује)...\n"

#, c-format
msgid "Enter application data (EOF to finish):\n"
msgstr "Унесите податке за обраду (<EOF> за крај):\n"

#, c-format
msgid "Base64 encoded application data to send:\n"
msgstr "Base64 кодовани подаци за слање:\n"

#, c-format
msgid "encoding error: %s"
msgstr "грешка кодовања: %s"

#, c-format
msgid "Session finished...\n"
msgstr "Сесија завршена...\n"

#, c-format
msgid "terminating GnuTLS session failed: %s"
msgstr "завршавање GnuTLS сесије неуспешно: %s"
