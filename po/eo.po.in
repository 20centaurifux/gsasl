#: src/gsasl.c:542
#, c-format
msgid "setting X.509 GnuTLS credential: %s"
msgstr "difino de X.509-legitimilo GnuTLS: %s"

#: src/gsasl.c:553
#, c-format
msgid "setting GnuTLS cipher priority (%s): %s\n"
msgstr "difino de ĉifra prioritato GnuTLS (%s): %s\n"

