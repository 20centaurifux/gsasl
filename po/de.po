#, c-format
msgid "Output from client:\n"
msgstr "Ausgabe vom Client:\n"

#: src/gsasl.c:152
#, c-format
msgid "Output from server:\n"
msgstr "Ausgabe vom Server:\n"

#: src/gsasl.c:304 src/gsasl.c:346 src/gsasl.c:397 src/gsasl.c:439
